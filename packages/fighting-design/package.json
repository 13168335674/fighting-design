{
  "name": "fighting-design",
  "version": "0.6.4-alpha.1",
  "description": "Fighting design can quickly build interactive interfaces in vue3 applications, which looks good.",
  "keywords": [
    "fighting",
    "design",
    "fighting-design",
    "vuejs",
    "components",
    "ui"
  ],
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./es/index.d.ts"
    },
    "./dist": "./dist/",
    "./theme": "./theme/",
    "./es": "./es/index.mjs",
    "./es/*": "./es/*.mjs",
    "./es/*.mjs": "./es/*.mjs",
    "./lib": "./lib/index.cjs",
    "./lib/*": "./lib/*.js",
    "./lib/*.cjs": "./lib/*.cjs",
    "./*": "./*"
  },
  "unpkg": "dist/index.umd.js",
  "style": "dist/index.css",
  "sideEffects": [
    "dist/*",
    "theme/*.css"
  ],
  "author": {
    "name": "tyh2001",
    "url": "https://tianyuhao.cn"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FightingDesign/fighting-design/issues"
  },
  "homepage": "https://fighting.tianyuhao.cn",
  "repository": "https://github.com/FightingDesign/fighting-design"
}